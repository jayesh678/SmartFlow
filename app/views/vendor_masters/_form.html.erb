<%= form_with model: @vendor_master do |form |%>
    <div>
    <%= form.label :customer_code %>
    <%= form.text_field :customer_code %>
    </div>

    <div>
    <%= form.label :customer_name %>
    <%= form.text_field :customer_name %>
    </div>

    <div>
    <%= form.label :corporate_number %>
    <%= form.text_field :corporate_number %>
    </div>

    <div>
    <%= form.label :invoice_number %>
    <%= form.text_field :invoice_number %>
    </div>

    <%= form.submit 'Add vendor master' %>
<% end %>